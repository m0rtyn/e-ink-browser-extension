{
  "manifest_version": 2,
  "name": "e-ink-browser-extension",
  "version": "1.0.0",
  "description": "Make your browser boring like a book ",
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": [
      "app.js"
    ]
  }]
}
