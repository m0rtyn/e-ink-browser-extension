{
  "description": "Make your browser boring like a book ",
  "name": "E-ink",
  "manifest_version": 2,
  "version": "1.0.0",
  "homepage_url": "https://github.com/martyns0n/e-ink-browser-extension",
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "browser_action": {
    "default_title": "Color boredom for focus"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "app.js"
      ],
      "run_at": "document_start"
    }
  ]
}